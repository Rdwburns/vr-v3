    "1626248705b8c25b91": {
      "type": "multicolumn-icons",
      "blocks": {
        "1626248705b8c25b91-0": {
          "type": "column",
          "settings": {
            "title": "Spot-Busting Formulation",
            "text": "<p>The fast-acting formula melts into skin treating spots at their core. Soothing skin from within calming inflammation, detoxifying pores, and reversing breakouts. <\/p>"
          }
        },
        "1626248705b8c25b91-1": {
          "type": "column",
          "settings": {
            "title": "Visible Results",
            "text": "<p>Designed to be tough on breakouts but gentle on skin with noticeable changes in the appearance of spots from just two hours.<\/p>"
          }
        },
        "1626248705b8c25b91-2": {
          "type": "column",
          "settings": {
            "title": "Precision Skincare",
            "text": "<p>Target and treat individual blemishes or breakouts with an easy to apply solution. Delivering active ingredients<br\/>below the surface of the skin, providing faster absorption and faster results.<\/p>"
          }
        }
      },
      "block_order": [
        "1626248705b8c25b91-0",
        "1626248705b8c25b91-1",
        "1626248705b8c25b91-2"
      ],
      "settings": {
        "title": "Key Benefits",
        "background_style": "secondary"
      }
    },
    "1626903216068cfb52": {
      "type": "before-after",
      "settings": {
        "heading": "Before and After",
        "review_title": "Eva, 30",
        "before_image": "shopify:\/\/shop_images\/Eva-Before.jpg",
        "before_caption": "Before",
        "after_image": "shopify:\/\/shop_images\/Eva-After.jpg",
        "after_caption": "After 1 Use",
        "height": "adapt",
        "color_scheme": "background-1",
        "layout": "image_first",
